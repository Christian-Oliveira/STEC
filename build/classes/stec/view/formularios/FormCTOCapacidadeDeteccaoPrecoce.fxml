<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="633.0" styleClass="card" stylesheets="@../../resources/css/style.css" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="stec.controller.FormCTOCapacidadeDeteccaoPrecoceController">
   <children>
