                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                  </JFXComboBox>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="200.0">
               <children>
                  <TabPane prefHeight="200.0" prefWidth="684.0" tabClosingPolicy="UNAVAILABLE">
