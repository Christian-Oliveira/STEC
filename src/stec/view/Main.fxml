<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" fx:id="MainPane" prefHeight="400.0" prefWidth="700.0" stylesheets="@../resources/css/style.css" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="stec.controller.MainController">
   <children>
      <VBox prefHeight="400.0" prefWidth="700.0">
         <children>
            <MenuBar>
              <menus>
                <Menu mnemonicParsing="false" text="Auditoria">
                  <items>
                        <MenuItem fx:id="mItemSupervisoes" mnemonicParsing="false" onAction="#handleMItemSupervisoes" text="Supervisões" />
                    <MenuItem fx:id="miNovaSupervisao" mnemonicParsing="false" onAction="#handleMINovaSupervisao" text="Nova supervisão" />
                        <MenuItem fx:id="miNovaSupervisaoPFFA" mnemonicParsing="false" onAction="#handleMINovaSupervisaoPFFA" text="Nova supervisão PFFA" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Análise">
                     <items>
                        <MenuItem fx:id="miRelatorios" mnemonicParsing="false" onAction="#handleMIRelatorios" text="Relatórios" />
                     </items></Menu>
                <Menu mnemonicParsing="false" text="Ajuda">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Sobre" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <AnchorPane fx:id="anchorPaneSupervisoes" prefHeight="375.0" prefWidth="700.0" />
         </children>
      </VBox>
   </children>
</AnchorPane>
